{
  "name": "@docs-islands/vitepress-docs",
  "type": "module",
  "scripts": {
    "dev": "vitepress dev",
    "build": "wait-on -d 100 ../dist/node/index.js && vitepress build",
    "preview": "vitepress preview",
    "link:dev": "npm pkg set devDependencies.@docs-islands/vitepress='link:../' && pnpm install && pnpm set:default-link",
    "link:prod": "pnpm set:default-link && pnpm install",
    "set:default-link": "npm pkg set devDependencies.@docs-islands/vitepress='link:../dist'"
  },
  "packageManager": "pnpm@10.7.0",
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "vue": "^3.5.21"
  },
  "devDependencies": {
    "@docs-islands/vitepress": "link:../dist",
    "@nolebase/vitepress-plugin-enhanced-mark": "^2.18.2",
    "@vitejs/plugin-react-swc": "^3.9.0",
    "vitepress": "^1.6.3",
    "vitepress-plugin-group-icons": "^1.6.3",
    "vitepress-plugin-llms": "^1.7.3",
    "wait-on": "^8.0.4"
  }
}
